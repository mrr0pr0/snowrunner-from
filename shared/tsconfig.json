{
    "compilerOptions": {
      "target": "ES2022",
      "lib": ["ES2022"],
      "module": "ESNext",
      "moduleResolution": "bundler",
      "outDir": "./dist",
      "rootDir": ".",
      "strict": true,
      "skipLibCheck": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "declaration": true,
      "declarationMap": true,
      "isolatedModules": true,
      "noFallthroughCasesInSwitch": true,
      "esModuleInterop": true,
      "resolveJsonModule": true
    },
    "include": ["./**/*.ts"]
  }
  